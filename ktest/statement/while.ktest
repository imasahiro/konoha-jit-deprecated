// while test

### while simple
>>> i = 5;
>>> while (i > 0) {
...   i--;
... }
>>> i
0

### while break
>>> i = 0;
>>> while (true) {
...   i++;
...   if (i == 10) break;
... }
>>> i
10

### while do-while
>>> i = 0;
>>> do {
...   i--;
... } while (i > 0);
>>> i
-1

### while nest
>>> i = 10;
>>> j = -10;
>>> res = 0;
>>> while (i > 0) {
...   while (j < 0) {
...     res++;
...     j++;
...   }
...   res++;
...   i--;
... }
>>> i
0
>>> j
0
>>> res
20
