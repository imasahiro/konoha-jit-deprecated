### switch multi case
>>> c = 'A';
>>> switch (c) {
... case 'A':
... case 'B':
...   res = "A or B";
...   break;
... default:
...   res = "not A or B";
...   break;
... }
>>> res
"A or B"
>>> switch (c) {
... case 'B':
...   res = "B";
...   break;
... case 'A':
... default:
...   res = "not B";
... }
>>> res
"not B"

### switch nest
>>> s = "ab";
>>> switch (s[0]) {
... case "a":
...   switch (s[1]) {
...   case "b":
...     res = "ab";
...   default:
...     res = "not ab";
...   }
... default:
...   res = "not ab";
... break;
... }
... res
"ab"
